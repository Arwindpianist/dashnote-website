{"ast":null,"code":"import { ParticlesBg } from 'particles-bg-vue';\nexport default {\n  name: 'MainPage',\n  components: {\n    ParticlesBg\n  },\n  data() {\n    return {\n      inMove: false,\n      inMoveDelay: 400,\n      activeSection: 0,\n      offsets: [],\n      touchStartY: 0\n    };\n  },\n  methods: {\n    calculateSectionOffsets() {\n      const sections = document.getElementsByClassName('scroll-section');\n      this.offsets = Array.from(sections).map(section => section.offsetTop);\n    },\n    handleMouseWheel(e) {\n      if (e.wheelDelta < 30 && !this.inMove) {\n        this.moveUp();\n      } else if (e.wheelDelta > 30 && !this.inMove) {\n        this.moveDown();\n      }\n      e.preventDefault();\n      return false;\n    },\n    handleMouseWheelDOM(e) {\n      if (e.detail > 0 && !this.inMove) {\n        this.moveUp();\n      } else if (e.detail < 0 && !this.inMove) {\n        this.moveDown();\n      }\n      return false;\n    },\n    moveDown() {\n      this.inMove = true;\n      this.activeSection--;\n      if (this.activeSection < 0) this.activeSection = this.offsets.length - 1;\n      this.scrollToSection(this.activeSection, true);\n    },\n    moveUp() {\n      this.inMove = true;\n      this.activeSection++;\n      if (this.activeSection > this.offsets.length - 1) this.activeSection = 0;\n      this.scrollToSection(this.activeSection, true);\n    },\n    scrollToSection(id, force = false) {\n      if (this.inMove && !force) return false;\n      this.activeSection = id;\n      this.inMove = true;\n      const section = document.getElementsByClassName('scroll-section')[id];\n      if (section) {\n        section.scrollIntoView({\n          behavior: 'smooth'\n        });\n      }\n      setTimeout(() => {\n        this.inMove = false;\n      }, this.inMoveDelay);\n    },\n    touchStart(e) {\n      e.preventDefault();\n      this.touchStartY = e.touches[0].clientY;\n    },\n    touchMove(e) {\n      if (this.inMove) return false;\n      e.preventDefault();\n      const currentY = e.touches[0].clientY;\n      if (this.touchStartY < currentY) {\n        this.moveDown();\n      } else {\n        this.moveUp();\n      }\n      this.touchStartY = 0;\n      return false;\n    }\n  },\n  mounted() {\n    this.calculateSectionOffsets();\n    window.addEventListener('DOMMouseScroll', this.handleMouseWheelDOM);\n    window.addEventListener('mousewheel', this.handleMouseWheel, {\n      passive: false\n    });\n    window.addEventListener('touchstart', this.touchStart, {\n      passive: false\n    });\n    window.addEventListener('touchmove', this.touchMove, {\n      passive: false\n    });\n  },\n  beforeUnmount() {\n    window.removeEventListener('DOMMouseScroll', this.handleMouseWheelDOM);\n    window.removeEventListener('mousewheel', this.handleMouseWheel, {\n      passive: false\n    });\n    window.removeEventListener('touchstart', this.touchStart);\n    window.removeEventListener('touchmove', this.touchMove);\n  }\n};","map":{"version":3,"names":["ParticlesBg","name","components","data","inMove","inMoveDelay","activeSection","offsets","touchStartY","methods","calculateSectionOffsets","sections","document","getElementsByClassName","Array","from","map","section","offsetTop","handleMouseWheel","e","wheelDelta","moveUp","moveDown","preventDefault","handleMouseWheelDOM","detail","length","scrollToSection","id","force","scrollIntoView","behavior","setTimeout","touchStart","touches","clientY","touchMove","currentY","mounted","window","addEventListener","passive","beforeUnmount","removeEventListener"],"sources":["/home/arwin/Desktop/code/dashnote/dashnote-website/src/components/MainPage.vue"],"sourcesContent":["<template>\n    <div>\n        <section v-for=\"(offset, index) in offsets\" :key=\"index\" :class=\"{ active: activeSection === index }\"\n            class=\"scroll-section\">\n            <div v-if=\"index === 0\" class=\"main\">\n                <div class=\"main-content\">\n                    <img src=\"../assets/dashnote.png\" alt=\"Dashnote Preview\">\n                    <div class=\"content-section\">\n                        <h1>Welcome to DashNote</h1>\n                        <p>DashNote is a Python note-taking app designed to simplify your note-taking experience.</p>\n                    </div>\n                </div>\n                <div class=\"bg\">\n                    <ParticlesBg type=\"lines\" num=\"100\" />\n                </div>\n            </div>\n\n            <div v-else class=\"main-content\">\n                <div class=\"content-section\">\n                    <h1>Features:</h1>\n                    <!-- Add other content specific to the Features section -->\n                </div>\n                <div class=\"bg2\">\n                    <ParticlesBg type=\"lines\" num=\"150\" />\n                </div>\n            </div>\n        </section>\n    </div>\n</template>\n  \n<script>\nimport { ParticlesBg } from 'particles-bg-vue';\n\nexport default {\n    name: 'MainPage',\n    components: {\n        ParticlesBg,\n    },\n    data() {\n        return {\n            inMove: false,\n            inMoveDelay: 400,\n            activeSection: 0,\n            offsets: [],\n            touchStartY: 0,\n        };\n    },\n    methods: {\n        calculateSectionOffsets() {\n            const sections = document.getElementsByClassName('scroll-section');\n            this.offsets = Array.from(sections).map((section) => section.offsetTop);\n        },\n        handleMouseWheel(e) {\n            if (e.wheelDelta < 30 && !this.inMove) {\n                this.moveUp();\n            } else if (e.wheelDelta > 30 && !this.inMove) {\n                this.moveDown();\n            }\n\n            e.preventDefault();\n            return false;\n        },\n        handleMouseWheelDOM(e) {\n            if (e.detail > 0 && !this.inMove) {\n                this.moveUp();\n            } else if (e.detail < 0 && !this.inMove) {\n                this.moveDown();\n            }\n\n            return false;\n        },\n        moveDown() {\n            this.inMove = true;\n            this.activeSection--;\n\n            if (this.activeSection < 0) this.activeSection = this.offsets.length - 1;\n\n            this.scrollToSection(this.activeSection, true);\n        },\n        moveUp() {\n            this.inMove = true;\n            this.activeSection++;\n\n            if (this.activeSection > this.offsets.length - 1) this.activeSection = 0;\n\n            this.scrollToSection(this.activeSection, true);\n        },\n        scrollToSection(id, force = false) {\n            if (this.inMove && !force) return false;\n\n            this.activeSection = id;\n            this.inMove = true;\n\n            const section = document.getElementsByClassName('scroll-section')[id];\n            if (section) {\n                section.scrollIntoView({ behavior: 'smooth' });\n            }\n\n            setTimeout(() => {\n                this.inMove = false;\n            }, this.inMoveDelay);\n        },\n        touchStart(e) {\n            e.preventDefault();\n\n            this.touchStartY = e.touches[0].clientY;\n        },\n        touchMove(e) {\n            if (this.inMove) return false;\n            e.preventDefault();\n\n            const currentY = e.touches[0].clientY;\n\n            if (this.touchStartY < currentY) {\n                this.moveDown();\n            } else {\n                this.moveUp();\n            }\n\n            this.touchStartY = 0;\n            return false;\n        },\n    },\n    mounted() {\n        this.calculateSectionOffsets();\n\n        window.addEventListener('DOMMouseScroll', this.handleMouseWheelDOM);\n        window.addEventListener('mousewheel', this.handleMouseWheel, { passive: false });\n\n        window.addEventListener('touchstart', this.touchStart, { passive: false });\n        window.addEventListener('touchmove', this.touchMove, { passive: false });\n    },\n    beforeUnmount() {\n        window.removeEventListener('DOMMouseScroll', this.handleMouseWheelDOM);\n        window.removeEventListener('mousewheel', this.handleMouseWheel, { passive: false });\n\n        window.removeEventListener('touchstart', this.touchStart);\n        window.removeEventListener('touchmove', this.touchMove);\n    },\n};\n</script>\n\n<style scoped>\n.main {\n    height: 80vh;\n    width: auto;\n    overflow: hidden;\n}\n\n.main-content {\n    margin: 0 auto;\n    width: 80%;\n    height: 80%;\n    display: flex;\n    flex-direction: row;\n    justify-content: center;\n    align-items: center;\n    font-size: larger;\n    z-index: 2;\n}\n\n.bg {\n    z-index: -1;\n    position: absolute;\n    top: 0;\n    left: 0;\n    width: 100%;\n    filter: blur(0.2rem) brightness(0.5);\n}\n\n.section {\n    width: 30vw;\n    margin-bottom: 40px;\n}\n\n.main-content img {\n    height: 20rem;\n    margin-right: 5rem;\n    border-radius: 0.2rem;\n}\n\n.front {\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n    align-items: center;\n    height: 100vh;\n    width: 100%;\n}\n\n.second {\n    height: 100vh;\n    width: 100%;\n    color: #1F538D;\n    background: none;\n}\n\n.bg2 {\n    z-index: -1;\n    position: absolute;\n    top: 100vh;\n    left: 0;\n    width: 100%;\n    filter: blur(0.2rem) brightness(0.5);\n}\n</style>\n"],"mappings":"AA+BA,SAASA,WAAU,QAAS,kBAAkB;AAE9C,eAAe;EACXC,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE;IACRF;EACJ,CAAC;EACDG,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,MAAM,EAAE,KAAK;MACbC,WAAW,EAAE,GAAG;MAChBC,aAAa,EAAE,CAAC;MAChBC,OAAO,EAAE,EAAE;MACXC,WAAW,EAAE;IACjB,CAAC;EACL,CAAC;EACDC,OAAO,EAAE;IACLC,uBAAuBA,CAAA,EAAG;MACtB,MAAMC,QAAO,GAAIC,QAAQ,CAACC,sBAAsB,CAAC,gBAAgB,CAAC;MAClE,IAAI,CAACN,OAAM,GAAIO,KAAK,CAACC,IAAI,CAACJ,QAAQ,CAAC,CAACK,GAAG,CAAEC,OAAO,IAAKA,OAAO,CAACC,SAAS,CAAC;IAC3E,CAAC;IACDC,gBAAgBA,CAACC,CAAC,EAAE;MAChB,IAAIA,CAAC,CAACC,UAAS,GAAI,EAAC,IAAK,CAAC,IAAI,CAACjB,MAAM,EAAE;QACnC,IAAI,CAACkB,MAAM,CAAC,CAAC;MACjB,OAAO,IAAIF,CAAC,CAAC