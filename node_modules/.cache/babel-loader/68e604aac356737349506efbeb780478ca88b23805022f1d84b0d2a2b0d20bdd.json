{"ast":null,"code":"import { ParticlesBg } from 'particles-bg-vue';\nexport default {\n  name: 'MainPage',\n  components: {\n    ParticlesBg\n  },\n  data() {\n    return {\n      inMove: false,\n      inMoveDelay: 400,\n      activeSection: 0,\n      offsets: [],\n      touchStartY: 0\n    };\n  },\n  methods: {\n    calculateSectionOffsets() {\n      const sections = document.getElementsByClassName('scroll-section');\n      this.offsets = Array.from(sections).map(section => section.offsetTop);\n    },\n    moveDown() {\n      this.inMove = true;\n      this.activeSection--;\n      if (this.activeSection < 0) this.activeSection = this.offsets.length - 1;\n      this.scrollToSection(this.activeSection, true);\n    },\n    moveUp() {\n      this.inMove = true;\n      this.activeSection++;\n      if (this.activeSection > this.offsets.length - 1) this.activeSection = 0;\n      this.scrollToSection(this.activeSection, true);\n    },\n    scrollToSection(id, force = false) {\n      if (this.inMove && !force) return false;\n      this.activeSection = id;\n      this.inMove = true;\n      const targetPosition = id * window.innerHeight;\n      window.scrollTo({\n        top: targetPosition,\n        behavior: 'smooth'\n      });\n      setTimeout(() => {\n        this.inMove = false;\n      }, this.inMoveDelay);\n    },\n    touchStart(e) {\n      e.preventDefault();\n      this.touchStartY = e.touches[0].clientY;\n    },\n    touchMove(e) {\n      if (this.inMove) return false;\n      e.preventDefault();\n      const currentY = e.touches[0].clientY;\n      if (this.touchStartY < currentY) {\n        this.moveDown();\n      } else {\n        this.moveUp();\n      }\n      this.touchStartY = 0;\n      return false;\n    }\n  },\n  mounted() {\n    this.calculateSectionOffsets();\n    window.addEventListener('DOMMouseScroll', this.handleMouseWheelDOM);\n    window.addEventListener('mousewheel', this.handleMouseWheel, {\n      passive: false\n    });\n    window.addEventListener('touchstart', this.touchStart, {\n      passive: false\n    });\n    window.addEventListener('touchmove', this.touchMove, {\n      passive: false\n    });\n  },\n  beforeUnmount() {\n    window.removeEventListener('DOMMouseScroll', this.handleMouseWheelDOM);\n    window.removeEventListener('mousewheel', this.handleMouseWheel, {\n      passive: false\n    });\n    window.removeEventListener('touchstart', this.touchStart);\n    window.removeEventListener('touchmove', this.touchMove);\n  }\n};","map":{"version":3,"names":["ParticlesBg","name","components","data","inMove","inMoveDelay","activeSection","offsets","touchStartY","methods","calculateSectionOffsets","sections","document","getElementsByClassName","Array","from","map","section","offsetTop","moveDown","length","scrollToSection","moveUp","id","force","targetPosition","window","innerHeight","scrollTo","top","behavior","setTimeout","touchStart","e","preventDefault","touches","clientY","touchMove","currentY","mounted","addEventListener","handleMouseWheelDOM","handleMouseWheel","passive","beforeUnmount","removeEventListener"],"sources":["/home/arwin/Desktop/code/dashnote/dashnote-website/src/components/MainPage.vue"],"sourcesContent":["<template>\n    <div>\n        <section v-for=\"(offset, index) in offsets\" :key=\"index\" :class=\"{ active: activeSection === index }\"\n            class=\"scroll-section\">\n            <div v-if=\"index === 0\" class=\"main\">\n                <div class=\"main-content\">\n                    <img src=\"../assets/dashnote.png\" alt=\"Dashnote Preview\">\n                    <div class=\"content-section section\">\n                        <h1>Welcome to DashNote</h1>\n                        <p>DashNote is a Python note-taking app designed to simplify your note-taking experience.</p>\n                    </div>\n                </div>\n                <div class=\"bg\">\n                    <ParticlesBg type=\"lines\" num=\"100\" />\n                </div>\n            </div>\n\n            <div v-else class=\"main-content\">\n                <div class=\"content-section section2\">\n                    <div class=\"feat\">\n                        <h1>Features:</h1>\n                        <p>\n                            DashNote is a simple note-taking application built using customtkinter. It allows you to create,\n                            save, and update notes with a user-friendly interface.\n                        </p>\n                    </div>\n                    <div class=\"feat-li\">\n                        <ul>\n                            <li>Dark theme for comfortable usage.</li>\n                            <li>Create new notes with a specified name.</li>\n                            <li>Save notes with automatic storage in a \"saved-notes\" folder.</li>\n                            <li>Update existing notes with ease.</li>\n                            <li>Left panel displays the history of saved notes.</li>\n                        </ul>\n                    </div>\n                </div>\n                <div class=\"bg2\">\n                    <ParticlesBg type=\"lines\" num=\"150\" />\n                </div>\n            </div>\n        </section>\n    </div>\n</template>\n  \n<script>\nimport { ParticlesBg } from 'particles-bg-vue';\n\nexport default {\n    name: 'MainPage',\n    components: {\n        ParticlesBg,\n    },\n    data() {\n        return {\n            inMove: false,\n            inMoveDelay: 400,\n            activeSection: 0,\n            offsets: [],\n            touchStartY: 0,\n        };\n    },\n    methods: {\n        calculateSectionOffsets() {\n            const sections = document.getElementsByClassName('scroll-section');\n            this.offsets = Array.from(sections).map((section) => section.offsetTop);\n        },\n        moveDown() {\n            this.inMove = true;\n            this.activeSection--;\n\n            if (this.activeSection < 0) this.activeSection = this.offsets.length - 1;\n\n            this.scrollToSection(this.activeSection, true);\n        },\n        moveUp() {\n            this.inMove = true;\n            this.activeSection++;\n\n            if (this.activeSection > this.offsets.length - 1) this.activeSection = 0;\n\n            this.scrollToSection(this.activeSection, true);\n        },\n        scrollToSection(id, force = false) {\n            if (this.inMove && !force) return false;\n\n            this.activeSection = id;\n            this.inMove = true;\n\n            const targetPosition = id * window.innerHeight;\n            window.scrollTo({\n                top: targetPosition,\n                behavior: 'smooth',\n            });\n\n            setTimeout(() => {\n                this.inMove = false;\n            }, this.inMoveDelay);\n        },\n        touchStart(e) {\n            e.preventDefault();\n\n            this.touchStartY = e.touches[0].clientY;\n        },\n        touchMove(e) {\n            if (this.inMove) return false;\n            e.preventDefault();\n\n            const currentY = e.touches[0].clientY;\n\n            if (this.touchStartY < currentY) {\n                this.moveDown();\n            } else {\n                this.moveUp();\n            }\n\n            this.touchStartY = 0;\n            return false;\n        },\n    },\n    mounted() {\n        this.calculateSectionOffsets();\n\n        window.addEventListener('DOMMouseScroll', this.handleMouseWheelDOM);\n        window.addEventListener('mousewheel', this.handleMouseWheel, { passive: false });\n\n        window.addEventListener('touchstart', this.touchStart, { passive: false });\n        window.addEventListener('touchmove', this.touchMove, { passive: false });\n    },\n    beforeUnmount() {\n        window.removeEventListener('DOMMouseScroll', this.handleMouseWheelDOM);\n        window.removeEventListener('mousewheel', this.handleMouseWheel, { passive: false });\n\n        window.removeEventListener('touchstart', this.touchStart);\n        window.removeEventListener('touchmove', this.touchMove);\n    },\n};\n</script>\n  \n<style scoped>\n/* Add your styling for scroll-sections here */\n</style>\n  "],"mappings":"AA6CA,SAASA,WAAU,QAAS,kBAAkB;AAE9C,eAAe;EACXC,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE;IACRF;EACJ,CAAC;EACDG,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,MAAM,EAAE,KAAK;MACbC,WAAW,EAAE,GAAG;MAChBC,aAAa,EAAE,CAAC;MAChBC,OAAO,EAAE,EAAE;MACXC,WAAW,EAAE;IACjB,CAAC;EACL,CAAC;EACDC,OAAO,EAAE;IACLC,uBAAuBA,CAAA,EAAG;MACtB,MAAMC,QAAO,GAAIC,QAAQ,CAACC,sBAAsB,CAAC,gBAAgB,CAAC;MAClE,IAAI,CAACN,OAAM,GAAIO,KAAK,CAACC,IAAI,CAACJ,QAAQ,CAAC,CAACK,GAAG,CAAEC,OAAO,IAAKA,OAAO,CAACC,SAAS,CAAC;IAC3E,CAAC;IACDC,QAAQA,CAAA,EAAG;MACP,IAAI,CAACf,MAAK,GAAI,IAAI;MAClB,IAAI,CAACE,aAAa,EAAE;MAEpB,IAAI,IAAI,CAACA,aAAY,GAAI,CAAC,EAAE,IAAI,CAACA,aAAY,GAAI,IAAI,CAACC,OAAO,CAACa,MAAK,GAAI,CAAC;MAExE,IAAI,CAACC,eAAe,CAAC,IAAI,CAACf,aAAa,EAAE,IAAI,CAAC;IAClD,CAAC;IACDgB,MAAMA,CAAA,EAAG;MACL,IAAI,CAAClB,MAAK,GAAI,IAAI;MAClB,IAAI,CAACE,aAAa,EAAE;MAEpB,IAAI,IAAI,CAACA,aAAY,GAAI,IAAI,CAACC,OAAO,CAACa,MAAK,GAAI,CAAC,EAAE,IAAI,CAACd,aAAY,GAAI,CAAC;MAExE,IAAI,CAACe,eAAe,CAAC,IAAI,CAACf,aAAa,EAAE,IAAI,CAAC;IAClD,CAAC;IACDe,eAAeA,CAACE,EAAE,EAAEC,KAAI,GAAI,KAAK,EAAE;MAC/B,IAAI,IAAI,CAACpB,MAAK,IAAK,CAACoB,KAAK,EAAE,OAAO,KAAK;MAEvC,IAAI,CAAClB,aAAY,GAAIiB,EAAE;MACvB,IAAI,CAACnB,MAAK,GAAI,IAAI;MAElB,MAAMqB,cAAa,GAAIF,EAAC,GAAIG,MAAM,CAACC,WAAW;MAC9CD,MAAM,CAACE,QAAQ,CAAC;QACZC,GAAG,EAAEJ,cAAc;QACnBK,QAAQ,EAAE;MACd,CAAC,CAAC;MAEFC,UAAU,CAAC,MAAM;QACb,IAAI,CAAC3B,MAAK,GAAI,KAAK;MACvB,CAAC,EAAE,IAAI,CAACC,WAAW,CAAC;IACxB,CAAC;IACD2B,UAAUA,CAACC,CAAC,EAAE;MACVA,CAAC,CAACC,cAAc,CAAC,CAAC;MAElB,IAAI,CAAC1B,WAAU,GAAIyB,CAAC,CAACE,OAAO,CAAC,CAAC,CAAC,CAACC,OAAO;IAC3C,CAAC;IACDC,SAASA,CAACJ,CAAC,EAAE;MACT,IAAI,IAAI,CAAC7B,MAAM,EAAE,OAAO,KAAK;MAC7B6B,CAAC,CAACC,cAAc,CAAC,CAAC;MAElB,MAAMI,QAAO,GAAIL,CAAC,CAACE,OAAO,CAAC,CAAC,CAAC,CAACC,OAAO;MAErC,IAAI,IAAI,CAAC5B,WAAU,GAAI8B,QAAQ,EAAE;QAC7B,IAAI,CAACnB,QAAQ,CAAC,CAAC;MACnB,OAAO;QACH,IAAI,CAACG,MAAM,CAAC,